doctype html
html(
	lang="en"
	ng-app="rpApp"
	ng-controller="rpAppCtrl"
	md-theme="{{theme}}"
	ng-class="{'rp-animation-false': !animations}"
	authenticated=`${authenticated}`
	user-agent=`${userAgent}`
	ng-init="init()"
)
	include head.pug

p userAgent: {{userAgent}}
p isGoogleBot: {{isGoogleBot}}

body.rp-body(
	layout="column"

)

	block facebook
	block content
